@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Manger/Views/_MangerLayout.cshtml";
    }
}
<script src="https://kit.fontawesome.com/ef3884b0e6.js" crossorigin="anonymous"></script>


<h2> إدارة حسابك</h2>

<div>
    <h4> تغيير إعدادات حسابك </h4>
    <hr />
    <div class="row">
        <div class="col-md-3">
            <partial name="_ManageNav" />
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
